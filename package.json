{
  "name": "cloudide-plus",
  "version": "0.1.0",
  "description": "A Powerful Cloud Development Environment Built on VS Code",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose up -d",
    "build": "npm run build:extensions",
    "build:extensions": "npm run build --workspaces",
    "install:extensions": "npm run install --workspaces",
    "package:extensions": "npm run package --workspaces",
    "test": "npm run test --workspaces",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "clean": "rimraf dist node_modules/.cache",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm install && npm run install:extensions",
    "start": "npm run docker:up"
  },
  "workspaces": [
    "cloudide-extensions/*"
  ],
  "keywords": [
    "vscode",
    "cloud-ide",
    "code-server",
    "cloud-development",
    "extensions",
    "docker",
    "google-cloud",
    "firebase",
    "cloudflare"
  ],
  "author": "CloudIDE+ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/CloudIDE.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/CloudIDE/issues"
  },
  "homepage": "https://github.com/yourusername/CloudIDE#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2"
  }
}

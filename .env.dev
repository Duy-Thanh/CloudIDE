# CloudIDE+ Development Environment Configuration
# Copy this file to .env and update the values as needed

# Coder Configuration
CODER_VERSION=latest
CODER_ACCESS_URL=http://localhost:7080

# Database Configuration
POSTGRES_USER=coder
POSTGRES_PASSWORD=coder123
POSTGRES_DB=coder

# Development Settings
NODE_ENV=development
DEBUG=true

# Future Cloud Service Keys (for later phases)
# Uncomment and fill these when you reach the appropriate development phase

# Google Cloud & Firebase (Phase 2)
# GOOGLE_CLOUD_PROJECT=your-project-id
# FIREBASE_PROJECT_ID=your-firebase-project
# GOOGLE_DRIVE_CLIENT_ID=your-client-id
# GOOGLE_DRIVE_CLIENT_SECRET=your-client-secret

# AI Services (Phase 3)
# GEMINI_API_KEY=your-gemini-api-key
# OPENAI_API_KEY=your-openai-api-key

# Deployment Services (Phase 2)
# VERCEL_TOKEN=your-vercel-token
# NETLIFY_TOKEN=your-netlify-token
# CLOUDFLARE_API_TOKEN=your-cloudflare-token

# AWS Services (Phase 2)
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# AWS_REGION=us-east-1

# Additional Storage APIs (Phase 2)
# DROPBOX_APP_KEY=your-dropbox-key
# DROPBOX_APP_SECRET=your-dropbox-secret
# ONEDRIVE_CLIENT_ID=your-onedrive-client-id
# ONEDRIVE_CLIENT_SECRET=your-onedrive-secret

# Customer Support (Phase 4)
# ZOHO_SALESIQ_TOKEN=your-zoho-token

# Security (Phase 4)
# RECAPTCHA_SITE_KEY=your-recaptcha-site-key
# RECAPTCHA_SECRET_KEY=your-recaptcha-secret-key

# Monitoring (Phase 4)
# GRAFANA_PASSWORD=admin
